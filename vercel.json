{\n  "version": 2,\n  "builds": [\n    {\n      "src": "client/package.json",\n      "use": "@vercel/static-build",\n      "config": {\n        "distDir": "client/build"\n      }\n    },\n    {\n      "src": "server/**/*.js",\n      "use": "@vercel/node"\n    }\n  ],\n  "routes": [\n    {\n      "src": "/(.*)",\n      "dest": "/client/build/$1"\n    },\n    {\n      "src": "/api/(.*)",\n      "dest": "/server/$1"\n    }\n  ]\n}